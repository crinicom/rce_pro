<%= div_for entrada do %>
<h5>
<% !entrada.nil? ? persona = User.find_by_id(entrada.persona_id) : "" %>
<%  tipo ||= TipoEntrada.find(entrada.tipo_entrada_id) if !entrada.tipo_entrada_id.nil?%>

<strong><%= !persona.nil? ? persona.name : "nombre persona?"%> </strong>&lt;<%= entrada.created_at.to_formatted_s(:short) %>&gt; <%= !tipo.nil? ? tipo.nombre : "" %>
<p class="time">
		Tiempo: <%= entrada.tiempo %>
	</p>
<span class='actions'>
<%= !@proyecto.nil? ? (link_to 'Delete', proyecto_entrada_path(:proyecto_id => @proyecto,
:id => entrada), confirm: 'Are you sure?', method: :delete) : "" %>
</span>
</h5>
<%= entrada.comentario %>

	
<br>

<% end %>