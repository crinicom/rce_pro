<%= div_for participante do %>
<h5><hr>
<% persona = Persona.find(participante.persona_id)%>
<% rol = Rol.find(participante.rol_id)%>

<%= link_to persona.nombre , edit_participante_path(participante) %>  <%= rol.nombre %>

<span class='actions'>
<%= link_to 'Delete', participante_path(participante), confirm: 'Are you sure?', method: :delete %>

</span>
</h5>

<% end %>