<%= div_for proyecto do %>
<h3>
<%= proyecto.nombre%> Horas previstas: <%= proyecto.horas%>
<span class='actions'>
<%= link_to 'Edit', edit_proyecto_path(@proyecto) %> |
<%= link_to 'Back', proyectos_path %>
<%= link_to "Delete", proyecto, confirm: "Are you sure?", method: :delete %>
</span>
</h3>
<% end %>